      "source": "ne-test",
      "source-layer": "bndl",
      "maxzoom": 5,
      "paint": {"line-color": "rgba(138, 138, 138, 1)"}
    },
    {
      "id": "popp",
      "type": "symbol",
      "source": "ne-test",
      "source-layer": "popp",
      "minzoom": 2,
      "maxzoom": 5,
      "layout": {
        "text-font": ["sans"],
        "text-field": "{NAME}",
