{
  "version": 8,
  "name": "test-simple",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "ne-test": {
      "type": "vector",
      "tiles": ["../ne-test/{z}/{x}/{y}.pbf"],
